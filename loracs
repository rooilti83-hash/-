local LogFrame
local LogText = ""
local MaxLogLines = 50

local function Log(message, color)
    color = color or Color3.fromRGB(255, 255, 255)
    local timestamp = os.date("%H:%M:%S")
    local logLine = "[" .. timestamp .. "] " .. message
    print(logLine)
    
    LogText = logLine .. "\n" .. LogText
    local lines = {}
    for line in LogText:gmatch("[^\n]+") do
        table.insert(lines, line)
        if #lines >= MaxLogLines then break end
    end
    LogText = table.concat(lines, "\n")
    
    if LogFrame and LogFrame:FindFirstChild("LogDisplay") then
        LogFrame.LogDisplay.Text = LogText
    end
end

local function AnalyzeWorkspace()
    Log("üîç –ê–ù–ê–õ–ò–ó WORKSPACE...", Color3.fromRGB(255, 255, 0))
    
    -- –ê–Ω–∞–ª–∏–∑ —Å—Ç—Ä—É–∫—Ç—É—Ä—ã workspace
    local bases = workspace:FindFirstChild("Bases")
    local map = workspace:FindFirstChild("Map")
    
    if bases then
        Log("‚úÖ –ù–∞–π–¥–µ–Ω–∞ –ø–∞–ø–∫–∞ Bases", Color3.fromRGB(0, 255, 0))
        for _, base in pairs(bases:GetChildren()) do
            Log("  ‚îî –ë–∞–∑–∞: " .. base.Name)
            local brainrotCount = 0
            for _, item in pairs(base:GetDescendants()) do
                if item:IsA("Model") then
                    brainrotCount = brainrotCount + 1
                    Log("    ‚îî –ú–æ–¥–µ–ª—å: " .. item.Name .. " (Parent: " .. item.Parent.Name .. ")")
                end
            end
            Log("  ‚îî –í—Å–µ–≥–æ –º–æ–¥–µ–ª–µ–π –≤ –±–∞–∑–µ: " .. brainrotCount)
        end
    else
        Log("‚ùå –ü–∞–ø–∫–∞ Bases –Ω–µ –Ω–∞–π–¥–µ–Ω–∞!", Color3.fromRGB(255, 0, 0))
    end
    
    if map then
        Log("‚úÖ –ù–∞–π–¥–µ–Ω–∞ Map", Color3.fromRGB(0, 255, 0))
        for _, child in pairs(map:GetChildren()) do
            Log("  ‚îî Map —Å–æ–¥–µ—Ä–∂–∏—Ç: " .. child.Name .. " (" .. child.ClassName .. ")")
        end
    end
    
    -- –ü–æ–∏—Å–∫ –∫–æ–Ω–≤–µ–π–µ—Ä–Ω–æ–π –ª–µ–Ω—Ç—ã [web:30][web:33]
    local belt = workspace:FindFirstChild("Belt") or workspace:FindFirstChild("Conveyor") or workspace:FindFirstChild("ConveyorBelt")
    if belt then
        Log("‚úÖ –ù–∞–π–¥–µ–Ω–∞ –∫–æ–Ω–≤–µ–π–µ—Ä–Ω–∞—è –ª–µ–Ω—Ç–∞: " .. belt.Name, Color3.fromRGB(0, 255, 0))
    else
        Log("‚ö†Ô∏è –ö–æ–Ω–≤–µ–π–µ—Ä–Ω–∞—è –ª–µ–Ω—Ç–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω–∞, –∏—â—É –∞–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤—ã...", Color3.fromRGB(255, 165, 0))
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj.Name:lower():find("belt") or obj.Name:lower():find("conveyor") then
                Log("  ‚îî –ù–∞–π–¥–µ–Ω: " .. obj:GetFullName())
            end
        end
    end
end

local function AnalyzeReplicatedStorage()
    Log("üîç –ê–ù–ê–õ–ò–ó REPLICATED STORAGE...", Color3.fromRGB(255, 255, 0))
    
    -- –ü–æ–∏—Å–∫ RemoteEvents –∏ RemoteFunctions [web:35]
    local remotes = {}
    for _, obj in pairs(ReplicatedStorage:GetDescendants()) do
        if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
            table.insert(remotes, obj)
            Log("üì° Remote –Ω–∞–π–¥–µ–Ω: " .. obj:GetFullName() .. " (" .. obj.ClassName .. ")", Color3.fromRGB(100, 200, 255))
        end
    end
    
    Log("‚úÖ –ù–∞–π–¥–µ–Ω–æ remotes: " .. #remotes, Color3.fromRGB(0, 255, 0))
    
    -- –ü–æ–∏—Å–∫ –≤–∞–∂–Ω—ã—Ö RemoteFunctions
    local spawnRemote = ReplicatedStorage:FindFirstChild("RF", true)
    if spawnRemote then
        Log("‚úÖ –ù–∞–π–¥–µ–Ω RF (RemoteFunction)", Color3.fromRGB(0, 255, 0))
        for _, child in pairs(spawnRemote:GetDescendants()) do
            Log("  ‚îî " .. child:GetFullName())
        end
    end
    
    return remotes
end

