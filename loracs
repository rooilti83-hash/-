-- ============================================
-- FLY MODE
-- ============================================

local flying = false
local flySpeed = 50
local bodyVelocity, bodyGyro

local function StartFly()
    if flying then return end
    flying = true
    
    bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.Velocity = Vector3.new(0, 0, 0)
    bodyVelocity.MaxForce = Vector3.new(9e4, 9e4, 9e4)
    bodyVelocity.Parent = hrp
    
    bodyGyro = Instance.new("BodyGyro")
    bodyGyro.MaxTorque = Vector3.new(9e4, 9e4, 9e4)
    bodyGyro.P = 9e4
    bodyGyro.Parent = hrp
    
    spawn(function()
        while flying and settings.flyMode do
            local camera = workspace.CurrentCamera
            local moveDirection = Vector3.new(0, 0, 0)
            
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                moveDirection = moveDirection + (camera.CFrame.LookVector * flySpeed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                moveDirection = moveDirection - (camera.CFrame.LookVector * flySpeed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                moveDirection = moveDirection - (camera.CFrame.RightVector * flySpeed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                moveDirection = moveDirection + (camera.CFrame.RightVector * flySpeed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                moveDirection = moveDirection + Vector3.new(0, flySpeed, 0)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                moveDirection = moveDirection - Vector3.new(0, flySpeed, 0)
            end
            
            bodyVelocity.Velocity = moveDirection
            bodyGyro.CFrame = camera.CFrame
            
            RunService.Heartbeat:Wait()
        end
    end)
end

local function StopFly()
    flying = false
    if bodyVelocity then bodyVelocity:Destroy() end
    if bodyGyro then bodyGyro:Destroy() end
end

-- ============================================
-- ESP –°–ò–°–¢–ï–ú–ê
-- ============================================

local function CreateBrainrotESP()
    for _, brainrot in pairs(foundBrainrots) do
        if brainrot.model and brainrot.model:IsDescendantOf(workspace) then
            if not brainrot.model:FindFirstChild("ESP_Highlight") then
                local highlight = Instance.new("Highlight")
                highlight.Name = "ESP_Highlight"
                highlight.Adornee = brainrot.model
                highlight.FillColor = Color3.fromRGB(255, 255, 0)
                highlight.FillTransparency = 0.5
                highlight.OutlineColor = Color3.fromRGB(255, 165, 0)
                highlight.OutlineTransparency = 0
                highlight.Parent = brainrot.model
            end
        end
    end
end

local function CreatePlayerESP()
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character then
            local char = otherPlayer.Character
            if not char:FindFirstChild("ESP_Highlight") then
                local highlight = Instance.new("Highlight")
                highlight.Name = "ESP_Highlight"
                highlight.Adornee = char
                highlight.FillColor = Color3.fromRGB(255, 0, 0)
                highlight.FillTransparency = 0.5
                highlight.OutlineColor = Color3.fromRGB(255, 255, 0)
                highlight.OutlineTransparency = 0
                highlight.Parent = char
            end
        end
    end
end

local function RemoveAllESP()
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj.Name == "ESP_Highlight" then
            obj:Destroy()
        end
    end
end

-- ============================================
-- AUTO LOCK BASE [web:33]
-- ============================================

local lastLockTime = 0
local function AutoLockBase()
    if not settings.autoLock then return end
    if tick() - lastLockTime < 70 then return end -- Cooldown 60 —Å–µ–∫—É–Ω–¥ + –∑–∞–ø–∞—Å [web:32][web:33]
    
    pcall(function()
        local bases = workspace:FindFirstChild("Bases")
        if not bases then return end
        
        local myBase = bases:FindFirstChild(player.Name)
        if not myBase then return end
        
        -- –ò—â–µ–º –∫–Ω–æ–ø–∫—É –±–ª–æ–∫–∏—Ä–æ–≤–∫–∏
        for _, obj in pairs(myBase:GetDescendants()) do
            if obj:IsA("ProximityPrompt") and (obj.Name:lower():find("lock") or obj.Parent.Name:lower():find("lock")) then
                fireproximityprompt(obj)
                lastLockTime = tick()
                Log("üîí –ë–∞–∑–∞ –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–∞ –Ω–∞ 60 —Å–µ–∫—É–Ω–¥", Color3.fromRGB(0, 255, 0))
                return
            end
            
            if obj:IsA("ClickDetector") and obj.Parent.Name:lower():find("lock") then
                fireclickdetector(obj)
                lastLockTime = tick()
                Log("üîí –ë–∞–∑–∞ –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–∞", Color3.fromRGB(0, 255, 0))
                return
            end
        end
    end)
end

-- ============================================
-- –ì–õ–ê–í–ù–´–ô –¶–ò–ö–õ
-- ============================================

local lastStealTime = 0
local lastAnalyzeTime = 0

RunService.Heartbeat:Connect(function()
    pcall(function()
        -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
        StatsLabel.Text = string.format("üìä –ü–æ–ø—ã—Ç–æ–∫: %d | ‚úÖ –£—Å–ø–µ—à–Ω–æ: %d | üéØ –ù–∞–π–¥–µ–Ω–æ: %d", 
            stealAttempts, successfulSteals, #foundBrainrots)
        
        -- Auto Analyze
        if settings.debugMode and tick() - lastAnalyzeTime > 30 then
            FindAllBrainrots()
            lastAnalyzeTime = tick()
        end
        
        -- Auto Steal
        if settings.autoSteal and tick() - lastStealTime > 2 then
            if #foundBrainrots == 0 then
                FindAllBrainrots()
            end
            
            if #foundBrainrots > 0 then
                local brainrot = foundBrainrots[1]
                if brainrot.model and brainrot.model:IsDescendantOf(workspace) then
                    StealBrainrot(brainrot)
                    table.remove(foundBrainrots, 1)
                end
            end
            
            lastStealTime = tick()
        end
        
        -- Auto Lock
        AutoLockBase()
        
        -- ESP
        if settings.esp then
            CreatePlayerESP()
            CreateBrainrotESP()
        else
            RemoveAllESP()
        end
        
        -- Fly
        if settings.flyMode and not flying then
            StartFly()
        elseif not settings.flyMode and flying then
            StopFly()
        end
        
        -- No Clip
        if settings.noClip then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)
end)

-- ============================================
-- –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø
-- ============================================

wait(2)
Log("üöÄ BRAINROT ADVANCED SCRIPT v4.0 –ó–ê–ì–†–£–ñ–ï–ù!", Color3.fromRGB(0, 255, 0))
Log("üìã –ù–∞—á–∏–Ω–∞—é –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π –∞–Ω–∞–ª–∏–∑...")
AnalyzeWorkspace()
wait(1)
remoteEvents = AnalyzeReplicatedStorage()
wait(1)
FindAllBrainrots()
Log("‚úÖ –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –∑–∞–≤–µ—Ä—à–µ–Ω–∞!", Color3.fromRGB(0, 255, 0))
Log("üí° –í–∫–ª—é—á–∏—Ç–µ Auto Steal –¥–ª—è –Ω–∞—á–∞–ª–∞ —Ä–∞–±–æ—Ç—ã")
