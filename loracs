-- –°–ò–°–¢–ï–ú–ê –ü–û–ò–°–ö–ê –ò –ê–ù–ê–õ–ò–ó–ê –ë–†–ï–ô–ù–†–û–¢–û–í
-- ============================================

local function FindAllBrainrots()
    foundBrainrots = {}
    
    -- –ú–µ—Ç–æ–¥ 1: –ü–æ–∏—Å–∫ –≤ Bases [web:29][web:30]
    local bases = workspace:FindFirstChild("Bases")
    if bases then
        for _, base in pairs(bases:GetChildren()) do
            if base.Name ~= player.Name then
                for _, item in pairs(base:GetDescendants()) do
                    if item:IsA("Model") and not item:IsA("Accessory") then
                        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —ç—Ç–æ –±—Ä–µ–π–Ω—Ä–æ—Ç –ø–æ —Ä–∞–∑–ª–∏—á–Ω—ã–º –ø—Ä–∏–∑–Ω–∞–∫–∞–º
                        local isBrainrot = false
                        
                        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 1: –ù–∞–∑–≤–∞–Ω–∏–µ —Å–æ–¥–µ—Ä–∂–∏—Ç —Ö–∞—Ä–∞–∫—Ç–µ—Ä–Ω—ã–µ —Å–ª–æ–≤–∞
                        local name = item.Name:lower()
                        if name:find("brain") or name:find("pizzanini") or name:find("cheese") 
                            or name:find("talpa") or name:find("tralala") or name:find("vacca") 
                            or name:find("noobini") or name:find("tim") then
                            isBrainrot = true
                        end
                        
                        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 2: –ï—Å—Ç—å ProximityPrompt
                        if item:FindFirstChildWhichIsA("ProximityPrompt", true) then
                            isBrainrot = true
                        end
                        
                        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ 3: –ï—Å—Ç—å ClickDetector
                        if item:FindFirstChildWhichIsA("ClickDetector", true) then
                            isBrainrot = true
                        end
                        
                        if isBrainrot then
                            table.insert(foundBrainrots, {
                                model = item,
                                base = base.Name,
                                position = item:GetModelCFrame().Position,
                                name = item.Name
                            })
                            Log("üéØ –ù–∞–π–¥–µ–Ω –±—Ä–µ–π–Ω—Ä–æ—Ç: " .. item.Name .. " –≤ –±–∞–∑–µ " .. base.Name)
                        end
                    end
                end
            end
        end
    end
    
    -- –ú–µ—Ç–æ–¥ 2: –ü–æ–∏—Å–∫ –Ω–∞ –∫–æ–Ω–≤–µ–π–µ—Ä–Ω–æ–π –ª–µ–Ω—Ç–µ [web:30][web:33]
    for _, obj in pairs(workspace:GetDescendants()) do
        if (obj.Name:lower():find("belt") or obj.Name:lower():find("conveyor")) and obj:IsA("Model") then
            for _, item in pairs(obj:GetDescendants()) do
                if item:IsA("Model") and item:FindFirstChild("PrimaryPart") then
                    table.insert(foundBrainrots, {
                        model = item,
                        base = "Belt",
                        position = item:GetModelCFrame().Position,
                        name = item.Name
                    })
                    Log("üéØ –ù–∞–π–¥–µ–Ω –±—Ä–µ–π–Ω—Ä–æ—Ç –Ω–∞ –ª–µ–Ω—Ç–µ: " .. item.Name)
                end
            end
        end
    end
    
    Log("‚úÖ –í—Å–µ–≥–æ –Ω–∞–π–¥–µ–Ω–æ –±—Ä–µ–π–Ω—Ä–æ—Ç–æ–≤: " .. #foundBrainrots, Color3.fromRGB(0, 255, 0))
    return foundBrainrots
end

-- ============================================
-- –ú–ù–û–ñ–ï–°–¢–í–ï–ù–ù–´–ï –ú–ï–¢–û–î–´ –ö–†–ê–ñ–ò
-- ============================================

-- –ú–µ—Ç–æ–¥ 1: ProximityPrompt [web:31]
local function StealMethod_ProximityPrompt(brainrot)
    pcall(function()
        stealAttempts = stealAttempts + 1
        Log("üîÑ –ü–æ–ø—ã—Ç–∫–∞ #" .. stealAttempts .. ": ProximityPrompt –Ω–∞ " .. brainrot.name)
        
        -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –∫ –±—Ä–µ–π–Ω—Ä–æ—Ç—É
        hrp.CFrame = brainrot.model:GetModelCFrame()
        wait(0.2)
        
        -- –ò—â–µ–º ProximityPrompt
        local prompt = brainrot.model:FindFirstChildWhichIsA("ProximityPrompt", true)
        if prompt then
            Log("  ‚îî ProximityPrompt –Ω–∞–π–¥–µ–Ω: " .. prompt.Name)
            fireproximityprompt(prompt)
            wait(0.3)
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —É—Å–ø–µ—Ö
            if brainrot.model.Parent ~= workspace and brainrot.model.Parent ~= brainrot.model.Parent then
                successfulSteals = successfulSteals + 1
                Log("‚úÖ –£–°–ü–ï–•! –£–∫—Ä–∞–¥–µ–Ω: " .. brainrot.name, Color3.fromRGB(0, 255, 0))
                return true
            end
        else
            Log("  ‚îî ProximityPrompt –Ω–µ –Ω–∞–π–¥–µ–Ω", Color3.fromRGB(255, 100, 0))
        end
    end)
    return false
end

-- –ú–µ—Ç–æ–¥ 2: ClickDetector [web:31]
local function StealMethod_ClickDetector(brainrot)
    pcall(function()
        stealAttempts = stealAttempts + 1
        Log("üîÑ –ü–æ–ø—ã—Ç–∫–∞ #" .. stealAttempts .. ": ClickDetector –Ω–∞ " .. brainrot.name)
        
        hrp.CFrame = brainrot.model:GetModelCFrame()
        wait(0.2)
        
        local click = brainrot.model:FindFirstChildWhichIsA("ClickDetector", true)
        if click then
            Log("  ‚îî ClickDetector –Ω–∞–π–¥–µ–Ω")
            fireclickdetector(click)
            wait(0.3)
            
            if not brainrot.model:IsDescendantOf(workspace) then
                successfulSteals = successfulSteals + 1
                Log("‚úÖ –£–°–ü–ï–• —á–µ—Ä–µ–∑ ClickDetector!", Color3.fromRGB(0, 255, 0))
                return true
            end
        else
            Log("  ‚îî ClickDetector –Ω–µ –Ω–∞–π–¥–µ–Ω", Color3.fromRGB(255, 100, 0))
        end
    end)
    return false
end

-- –ú–µ—Ç–æ–¥ 3: –ö–∞—Å–∞–Ω–∏–µ (Touch) [web:31][web:36]
local function StealMethod_Touch(brainrot)
    pcall(function()
        stealAttempts = stealAttempts + 1
        Log("üîÑ –ü–æ–ø—ã—Ç–∫–∞ #" .. stealAttempts .. ": –ö–∞—Å–∞–Ω–∏–µ " .. brainrot.name)
        
        -- –ù–∞—Ö–æ–¥–∏–º —á–∞—Å—Ç–∏ –¥–ª—è –∫–∞—Å–∞–Ω–∏—è
        local parts = {}
        for _, part in pairs(brainrot.model:GetDescendants()) do
            if part:IsA("BasePart") then
                table.insert(parts, part)
            end
        end
        
        if #parts > 0 then
            Log("  ‚îî –ù–∞–π–¥–µ–Ω–æ —á–∞—Å—Ç–µ–π –¥–ª—è –∫–∞—Å–∞–Ω–∏—è: " .. #parts)
            
            -- –ö–∞—Å–∞–µ–º—Å—è –∫–∞–∂–¥–æ–π —á–∞—Å—Ç–∏
            for _, part in pairs(parts) do
                hrp.CFrame = part.CFrame
                wait(0.1)
                
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –±—ã–ª –ª–∏ —Ç—Ä–∏–≥–≥–µ—Ä
                if not brainrot.model:IsDescendantOf(workspace) then
                    successfulSteals = successfulSteals + 1
                    Log("‚úÖ –£–°–ü–ï–• —á–µ—Ä–µ–∑ –∫–∞—Å–∞–Ω–∏–µ!", Color3.fromRGB(0, 255, 0))
                    return true
                end
            end
        end
    end)
    return false
end

-- –ú–µ—Ç–æ–¥ 4: RemoteEvent [web:35]
local function StealMethod_Remote(brainrot)
    pcall(function()
        stealAttempts = stealAttempts + 1
        Log("üîÑ –ü–æ–ø—ã—Ç–∫–∞ #" .. stealAttempts .. ": RemoteEvent –Ω–∞ " .. brainrot.name)
        
        -- –ò—â–µ–º remotes —Å–≤—è–∑–∞–Ω–Ω—ã–µ —Å –∫—Ä–∞–∂–µ–π
        for _, remote in pairs(remoteEvents) do
            if remote:IsA("RemoteEvent") then
                local remoteName = remote.Name:lower()
                if remoteName:find("steal") or remoteName:find("take") or remoteName:find("grab") 
                    or remoteName:find("pickup") or remoteName:find("collect") then
                    
                    Log("  ‚îî –ü—Ä–æ–±—É–µ–º remote: " .. remote.Name)
                    
                    -- –ü—Ä–æ–±—É–µ–º —Ä–∞–∑–Ω—ã–µ –∞—Ä–≥—É–º–µ–Ω—Ç—ã
                    remote:FireServer(brainrot.model)
                    wait(0.1)
                    remote:FireServer(brainrot.model.Name)
                    wait(0.1)
                    remote:FireServer(brainrot.model, player)
                    wait(0.1)
                    
                    if not brainrot.model:IsDescendantOf(workspace) then
                        successfulSteals = successfulSteals + 1
                        Log("‚úÖ –£–°–ü–ï–• —á–µ—Ä–µ–∑ Remote: " .. remote.Name, Color3.fromRGB(0, 255, 0))
                        return true
                    end
                end
            end
        end
    end)
    return false
end

-- –ú–µ—Ç–æ–¥ 5: Parent Manipulation [web:31]
local function StealMethod_ParentManipulation(brainrot)
    pcall(function()
        stealAttempts = stealAttempts + 1
        Log("üîÑ –ü–æ–ø—ã—Ç–∫–∞ #" .. stealAttempts .. ": Parent Manipulation –Ω–∞ " .. brainrot.name)
        
        hrp.CFrame = brainrot.model:GetModelCFrame()
        wait(0.2)
        
        -- –ü—Ä–æ–±—É–µ–º –∏–∑–º–µ–Ω–∏—Ç—å parent
        local originalParent = brainrot.model.Parent
        
        -- –ú–µ—Ç–æ–¥ 1: –ü–µ—Ä–µ–º–µ—Å—Ç–∏—Ç—å –∫ —Å–µ–±–µ
        brainrot.model.Parent = character
        wait(0.2)
        
        if brainrot.model.Parent == character then
            successfulSteals = successfulSteals + 1
            Log("‚úÖ –£–°–ü–ï–•! –ë—Ä–µ–π–Ω—Ä–æ—Ç –ø–µ—Ä–µ–º–µ—â–µ–Ω –∫ –ø–µ—Ä—Å–æ–Ω–∞–∂—É", Color3.fromRGB(0, 255, 0))
            
            -- –ü–µ—Ä–µ–Ω–æ—Å–∏–º –≤ —Å–≤–æ—é –±–∞–∑—É
            local bases = workspace:FindFirstChild("Bases")
            if bases then
                local myBase = bases:FindFirstChild(player.Name)
                if myBase then
                    brainrot.model.Parent = myBase
                    brainrot.model:MoveTo(myBase:GetModelCFrame().Position)
                    Log("‚úÖ –ë—Ä–µ–π–Ω—Ä–æ—Ç –ø–µ—Ä–µ–º–µ—â–µ–Ω –≤ —Å–≤–æ—é –±–∞–∑—É!")
                end
            end
            return true
        end
        
        -- –ú–µ—Ç–æ–¥ 2: –ß–µ—Ä–µ–∑ workspace
        brainrot.model.Parent = workspace
        wait(0.1)
        brainrot.model:MoveTo(hrp.Position)
        wait(0.2)
    end)
    return false
end

-- –ú–µ—Ç–æ–¥ 6: Teleport and Grab [web:31][web:36]
local function StealMethod_TeleportGrab(brainrot)
    pcall(function()
        stealAttempts = stealAttempts + 1
        Log("üîÑ –ü–æ–ø—ã—Ç–∫–∞ #" .. stealAttempts .. ": Teleport + Multiple Grabs –Ω–∞ " .. brainrot.name)
        
        -- –ë—ã—Å—Ç—Ä–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è —Ç—É–¥–∞-—Å—é–¥–∞
        local originalCFrame = hrp.CFrame
        
        for i = 1, 5 do
            hrp.CFrame = brainrot.model:GetModelCFrame()
            wait(0.05)
            
            -- –ü—Ä–æ–±—É–µ–º –≤—Å–µ –º–µ—Ç–æ–¥—ã –≤–∑–∞–∏–º–æ–¥–µ–π—Å—Ç–≤–∏—è –æ–¥–Ω–æ–≤—Ä–µ–º–µ–Ω–Ω–æ
            local prompt = brainrot.model:FindFirstChildWhichIsA("ProximityPrompt", true)
            if prompt then fireproximityprompt(prompt) end
            
            local click = brainrot.model:FindFirstChildWhichIsA("ClickDetector", true)
            if click then fireclickdetector(click) end
            
            wait(0.05)
            
            if not brainrot.model:IsDescendantOf(workspace) then
                successfulSteals = successfulSteals + 1
                Log("‚úÖ –£–°–ü–ï–• –ø–æ—Å–ª–µ " .. i .. " –ø–æ–ø—ã—Ç–æ–∫!", Color3.fromRGB(0, 255, 0))
                return true
            end
        end
    end)
    return false
end

-- –ì–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –∫—Ä–∞–∂–∏
local function StealBrainrot(brainrot)
    Log("üéØ –ù–ê–ß–ê–õ–û –ö–†–ê–ñ–ò: " .. brainrot.name .. " –∏–∑ –±–∞–∑—ã " .. brainrot.base)
    
    local methods = {
        {name = "ProximityPrompt", func = StealMethod_ProximityPrompt},
        {name = "ClickDetector", func = StealMethod_ClickDetector},
        {name = "Touch", func = StealMethod_Touch},
        {name = "RemoteEvent", func = StealMethod_Remote},
        {name = "ParentManipulation", func = StealMethod_ParentManipulation},
        {name = "TeleportGrab", func = StealMethod_TeleportGrab}
    }
    
    -- –ü—Ä–æ–±—É–µ–º –∫–∞–∂–¥—ã–π –º–µ—Ç–æ–¥
    for _, method in ipairs(methods) do
        Log("üìã –ú–µ—Ç–æ–¥: " .. method.name)
        local success = method.func(brainrot)
        if success then
            Log("‚úÖ –ú–µ—Ç–æ–¥ " .. method.name .. " –°–†–ê–ë–û–¢–ê–õ!", Color3.fromRGB(0, 255, 0))
            return true
        end
        wait(0.2)
    end
    
    Log("‚ùå –í—Å–µ –º–µ—Ç–æ–¥—ã –ø—Ä–æ–≤–∞–ª–∏–ª–∏—Å—å –¥–ª—è " .. brainrot.name, Color3.fromRGB(255, 0, 0))
    return false
end

