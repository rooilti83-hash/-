-- –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –¥–ª—è –∫–Ω–æ–ø–æ–∫
local ButtonContainer = Instance.new("ScrollingFrame")
ButtonContainer.Size = UDim2.new(1, -20, 1, -70)
ButtonContainer.Position = UDim2.new(0, 10, 0, 60)
ButtonContainer.BackgroundTransparency = 1
ButtonContainer.BorderSizePixel = 0
ButtonContainer.ScrollBarThickness = 6
ButtonContainer.CanvasSize = UDim2.new(0, 0, 0, 750)
ButtonContainer.Parent = MainFrame

-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –∫–Ω–æ–ø–∫–∏-–ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—è
local function CreateToggleButton(name, yPos, callback)
    local button = Instance.new("TextButton")
    button.Name = name
    button.Size = UDim2.new(1, -10, 0, 45)
    button.Position = UDim2.new(0, 5, 0, yPos)
    button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    button.Text = name .. ": OFF"
    button.TextColor3 = Color3.fromRGB(200, 200, 200)
    button.Font = Enum.Font.Gotham
    button.TextSize = 14
    button.Parent = ButtonContainer
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = button
    
    local enabled = false
    button.MouseButton1Click:Connect(function()
        enabled = not enabled
        button.Text = name .. (enabled and ": ON" or ": OFF")
        button.BackgroundColor3 = enabled and Color3.fromRGB(0, 170, 80) or Color3.fromRGB(50, 50, 50)
        button.TextColor3 = enabled and Color3.fromRGB(255, 255, 255) or Color3.fromRGB(200, 200, 200)
        callback(enabled)
    end)
    
    return button
end

-- ===================== BYPASS LOCKED BASE –°–ò–°–¢–ï–ú–ê =====================
-- –ú–µ—Ç–æ–¥ 1: Quantum Cloner + Invisibility Bypass [web:17][web:28]
local function QuantumClonerBypass(targetBaseName)
    pcall(function()
        local bases = workspace:FindFirstChild("Bases")
        if not bases then return end
        
        local enemyBase = bases:FindFirstChild(targetBaseName)
        if not enemyBase then return end
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –µ—Å—Ç—å –ª–∏ Quantum Cloner –≤ –∏–Ω–≤–µ–Ω—Ç–∞—Ä–µ
        local hasCloner = false
        for _, tool in pairs(player.Backpack:GetChildren()) do
            if tool.Name:lower():find("cloner") or tool.Name:lower():find("quantum") then
                hasCloner = true
                tool.Parent = character
                wait(0.1)
                break
            end
        end
        
        if hasCloner then
            -- –ö–ª–æ–Ω–∏—Ä—É–µ–º—Å—è –≤–Ω—É—Ç—Ä—å –±–∞–∑—ã
            local baseCenter = enemyBase:GetModelCFrame()
            hrp.CFrame = baseCenter + Vector3.new(0, 5, 0)
            wait(0.3)
            
            -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º –Ω–µ–≤–∏–¥–∏–º–æ—Å—Ç—å –µ—Å–ª–∏ –µ—Å—Ç—å
            for _, tool in pairs(player.Backpack:GetChildren()) do
                if tool.Name:lower():find("invisibility") or tool.Name:lower():find("cloak") then
                    tool.Parent = character
                    wait(0.1)
                    if tool:FindFirstChild("Handle") then
                        tool:Activate()
                    end
                    break
                end
            end
            
            insideEnemyBase = true
            print("‚úÖ –ü—Ä–æ–Ω–∏–∫–ª–∏ –≤ –±–∞–∑—É —á–µ—Ä–µ–∑ Quantum Cloner!")
        end
    end)
end

-- –ú–µ—Ç–æ–¥ 2: Admin Panel Method - –∫–æ–Ω—Ç—Ä–æ–ª—å –≤–ª–∞–¥–µ–ª—å—Ü–∞ [web:17]
local function AdminPanelBypass(targetPlayer)
    pcall(function()
        local adminPanel = player.Backpack:FindFirstChild("Admin Panel") or character:FindFirstChild("Admin Panel")
        if adminPanel then
            adminPanel.Parent = character
            wait(0.1)
            
            -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º –∫–æ–Ω—Ç—Ä–æ–ª—å –Ω–∞–¥ –≤–ª–∞–¥–µ–ª—å—Ü–µ–º –±–∞–∑—ã
            local remotes = ReplicatedStorage:GetDescendants()
            for _, remote in pairs(remotes) do
                if remote:IsA("RemoteEvent") and remote.Name:lower():find("admin") then
                    remote:FireServer("Control", targetPlayer)
                    print("‚úÖ –ö–æ–Ω—Ç—Ä–æ–ª–∏—Ä—É–µ–º –≤–ª–∞–¥–µ–ª—å—Ü–∞ –±–∞–∑—ã —á–µ—Ä–µ–∑ Admin Panel!")
                    wait(5) -- –î–µ—Ä–∂–∏–º –∫–æ–Ω—Ç—Ä–æ–ª—å 5 —Å–µ–∫—É–Ω–¥
                    remote:FireServer("Release", targetPlayer)
                end
            end
        end
    end)
end

-- –ú–µ—Ç–æ–¥ 3: Wall Bypass —á–µ—Ä–µ–∑ Collision [web:17][web:28]
local function WallBypass(targetBase)
    pcall(function()
        -- –û—Ç–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é —Å–æ —Å—Ç–µ–Ω–∞–º–∏ –±–∞–∑—ã
        for _, part in pairs(targetBase:GetDescendants()) do
            if part:IsA("BasePart") and (part.Name:lower():find("wall") or part.Name:lower():find("door") or part.Name:lower():find("laser")) then
                part.CanCollide = false
                part.Transparency = 0.5
            end
        end
        
        -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –≤–Ω—É—Ç—Ä—å
        hrp.CFrame = targetBase:GetModelCFrame() + Vector3.new(0, 5, 0)
        print("‚úÖ –ü—Ä–æ—à–ª–∏ —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã –±–∞–∑—ã!")
    end)
end

-- –ú–µ—Ç–æ–¥ 4: Body Swap + Trap Bypass [web:17]
local function BodySwapBypass(targetPlayer)
    pcall(function()
        local bodySwap = player.Backpack:FindFirstChild("Body Swap") or character:FindFirstChild("Body Swap")
        if not bodySwap then return end
        
        -- –°—Ç–∞–≤–∏–º –ª–æ–≤—É—à–∫–∏ –≤–æ–∑–ª–µ –±–∞–∑—ã –∂–µ—Ä—Ç–≤—ã
        local trap = player.Backpack:FindFirstChild("Trap")
        if trap then
            trap.Parent = character
            wait(0.1)
            
            local bases = workspace:FindFirstChild("Bases")
            if bases then
                local enemyBase = bases:FindFirstChild(targetPlayer.Name)
                if enemyBase then
                    -- –°—Ç–∞–≤–∏–º 4 –ª–æ–≤—É—à–∫–∏ —Å–∑–∞–¥–∏ –±–∞–∑—ã
                    local backPos = enemyBase:GetModelCFrame() * CFrame.new(0, 0, 10)
                    for i = 1, 4 do
                        if trap then
                            trap:Activate()
                            wait(0.5)
                        end
                    end
                    
                    -- –°–≤–∞–ø–∞–µ–º—Å—è —Å –≤–ª–∞–¥–µ–ª—å—Ü–µ–º –∫–æ–≥–¥–∞ –æ–Ω –ø–æ–ø—ã—Ç–∞–µ—Ç—Å—è –∑–∞–∫—Ä—ã—Ç—å –±–∞–∑—É
                    bodySwap.Parent = character
                    wait(0.1)
                    bodySwap:Activate()
                    print("‚úÖ –ó–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–ª–∏ –≤–ª–∞–¥–µ–ª—å—Ü–∞ –≤ –ª–æ–≤—É—à–∫–∞—Ö!")
                end
            end
        end
    end)
end

-- –ú–µ—Ç–æ–¥ 5: Instant Teleport Exploit [web:23]
local function InstantTeleportBypass(targetBase)
    pcall(function()
        -- –ú–≥–Ω–æ–≤–µ–Ω–Ω–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –≤–Ω—É—Ç—Ä—å –±–∞–∑—ã –º–∏–Ω—É—è –≤—Å–µ –ø—Ä–æ–≤–µ—Ä–∫–∏
        local oldCFrame = hrp.CFrame
        
        for i = 1, 3 do
            hrp.CFrame = targetBase:GetModelCFrame() + Vector3.new(0, 3, 0)
            wait(0.1)
        end
        
        -- –û—Ç–∫–ª—é—á–∞–µ–º –≤—Å–µ —Ç—Ä–∏–≥–≥–µ—Ä—ã –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏
        for _, trigger in pairs(targetBase:GetDescendants()) do
            if trigger:IsA("Part") and trigger.Name:lower():find("trigger") then
                trigger:Destroy()
            end
        end
        
        print("‚úÖ –ú–≥–Ω–æ–≤–µ–Ω–Ω–æ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–ª–∏—Å—å –≤ –±–∞–∑—É!")
    end)
end

-- –ú–µ—Ç–æ–¥ 6: Anti-Owner Control [web:17]
local function AntiOwnerControl(targetPlayer)
    pcall(function()
        if not targetPlayer.Character then return end
        
        local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
        if targetHRP then
            -- –ò—Å–ø–æ–ª—å–∑—É–µ–º Medusa Head –¥–ª—è –∑–∞–º–æ—Ä–æ–∑–∫–∏ –≤–ª–∞–¥–µ–ª—å—Ü–∞
            local medusa = player.Backpack:FindFirstChild("Medusa Head") or character:FindFirstChild("Medusa Head")
            if medusa then
                medusa.Parent = character
                wait(0.1)
                
                -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º Medusa –Ω–∞ –≤–ª–∞–¥–µ–ª—å—Ü–∞
                hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 0, 5)
                wait(0.1)
                medusa:Activate()
                
                print("‚úÖ –ó–∞–º–æ—Ä–æ–∑–∏–ª–∏ –≤–ª–∞–¥–µ–ª—å—Ü–∞ –±–∞–∑—ã Medusa Head!")
                
                -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º –µ–≥–æ –ø–æ–¥–∞–ª—å—à–µ –æ—Ç –±–∞–∑—ã —Å –ø–æ–º–æ—â—å—é Webslinger
                wait(1)
                local webslinger = player.Backpack:FindFirstChild("Webslinger")
                if webslinger then
                    webslinger.Parent = character
                    wait(0.1)
                    webslinger:Activate()
                    print("‚úÖ –û—Ç—Ç–∞—â–∏–ª–∏ –≤–ª–∞–¥–µ–ª—å—Ü–∞ –æ—Ç –±–∞–∑—ã!")
                end
            end
        end
    end)
end

-- –ì–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –æ–±—Ö–æ–¥–∞ –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–Ω–æ–π –±–∞–∑—ã
local function BypassLockedBase()
    if not settings.bypassLock then return end
    
    local bases = workspace:FindFirstChild("Bases")
    if not bases then return end
    
    -- –ù–∞—Ö–æ–¥–∏–º –ª—É—á—à—É—é –±–∞–∑—É –¥–ª—è –æ–≥—Ä–∞–±–ª–µ–Ω–∏—è
    for _, base in pairs(bases:GetChildren()) do
        if base.Name ~= player.Name then
            pcall(function()
                local brainrotCount = 0
                for _, item in pairs(base:GetDescendants()) do
                    if item:IsA("Model") and item.Name:lower():find("brainrot") then
                        brainrotCount = brainrotCount + 1
                    end
                end
                
                -- –ï—Å–ª–∏ –≤ –±–∞–∑–µ –±–æ–ª—å—à–µ 3 –±—Ä–µ–π–Ω—Ä–æ—Ç–æ–≤ - –∞—Ç–∞–∫—É–µ–º
                if brainrotCount >= 3 then
                    targetBase = base
                    local targetPlayer = Players:FindFirstChild(base.Name)
                    
                    print("üéØ –¶–µ–ª—å –Ω–∞–π–¥–µ–Ω–∞: " .. base.Name .. " (" .. brainrotCount .. " brainrots)")
                    
                    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –≤—Å–µ –º–µ—Ç–æ–¥—ã –æ–±—Ö–æ–¥–∞ –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ
                    
                    -- 1. –°–Ω–∞—á–∞–ª–∞ –∫–æ–Ω—Ç—Ä–æ–ª–∏—Ä—É–µ–º –≤–ª–∞–¥–µ–ª—å—Ü–∞ –µ—Å–ª–∏ –æ–Ω –æ–Ω–ª–∞–π–Ω
                    if targetPlayer and settings.antiOwner then
                        AntiOwnerControl(targetPlayer)
                        wait(1)
                    end
                    
                    -- 2. –ö–ª–æ–Ω–∏—Ä—É–µ–º—Å—è –≤–Ω—É—Ç—Ä—å –±–∞–∑—ã
                    QuantumClonerBypass(base.Name)
                    wait(0.5)
                    
                    -- 3. –ï—Å–ª–∏ –Ω–µ –ø–æ–ª—É—á–∏–ª–æ—Å—å - –∏—Å–ø–æ–ª—å–∑—É–µ–º wall bypass
                    if not insideEnemyBase then
                        WallBypass(base)
                        wait(0.5)
                    end
                    
                    -- 4. –ï—Å–ª–∏ –≤–ª–∞–¥–µ–ª–µ—Ü –æ–Ω–ª–∞–π–Ω - –∏—Å–ø–æ–ª—å–∑—É–µ–º Body Swap –º–µ—Ç–æ–¥
                    if targetPlayer then
                        BodySwapBypass(targetPlayer)
                        wait(0.5)
                    end
                    
                    -- 5. –ö—Ä–∞–π–Ω–∏–π —Å–ª—É—á–∞–π - instant teleport
                    if not insideEnemyBase then
                        InstantTeleportBypass(base)
                    end
                    
                    -- 6. –ö—Ä–∞—Å—Ç—å –≤—Å–µ —Ü–µ–Ω–Ω–æ–µ
                    wait(1)
                    for _, brainrot in pairs(base:GetDescendants()) do
                        if brainrot:IsA("Model") and brainrot.Name:lower():find("brainrot") then
                            if settings.instantSteal then
                                -- –ú–≥–Ω–æ–≤–µ–Ω–Ω–∞—è –∫—Ä–∞–∂–∞
                                brainrot.Parent = workspace
                                wait(0.1)
                                brainrot:MoveTo(hrp.Position)
                            else
                                hrp.CFrame = brainrot:GetModelCFrame()
                                wait(0.3)
                                fireproximityprompt(brainrot:FindFirstChildWhichIsA("ProximityPrompt", true))
                            end
                            wait(0.2)
                        end
                    end
                    
                    print("‚úÖ –û–≥—Ä–∞–±–ª–µ–Ω–∏–µ –∑–∞–≤–µ—Ä—à–µ–Ω–æ!")
                    insideEnemyBase = false
                    
                    -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –Ω–∞ —Å–≤–æ—é –±–∞–∑—É
                    local myBase = bases:FindFirstChild(player.Name)
                    if myBase then
                        hrp.CFrame = myBase:GetModelCFrame() + Vector3.new(0, 10, 0)
                    end
                    
                    wait(5) -- –ó–∞–¥–µ—Ä–∂–∫–∞ –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–µ–π –∞—Ç–∞–∫–æ–π
                end
            end)
        end
    end
end

